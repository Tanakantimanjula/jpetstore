node{
    stage("checkout")
    checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: '3cfd0d05-ba0b-4548-9c6e-2fdfe649f148', url: 'https://github.com/Tanakantimanjula/jpetstore']]])
}
stage("build")
{
    sh label:'',script:"mvn install"
}
